 class Solution {
public:
vector<int> twoSum(vector<int>& nums, int target) {
    int n=nums.size();
    vector<int>ans;
    unordered_map<int,int>umap;
    for(int i=0;i<n;i++)
    {
        if(umap.count(target-nums[i])){
            ans.push_back(umap[target-nums[i]]);
            ans.push_back(i);
        }
        umap[nums[i]]=i;
    }
    return ans;
  }
};
